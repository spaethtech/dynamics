{
    "name": "rspaeth/dynamics",
    "description": "A library for dynamic objects and methods.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "rspaeth/common": "*",
        "rspaeth/annotations": "*",
        "rspaeth/collections": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rspaeth\\": "./tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
